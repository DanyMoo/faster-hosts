{
    "name": "React-FasterHosts",
    "version": "0.3.0",
    "manifest_version": 2,
    "icons": {
    },
    "description": "加快访问",
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "webRequest", "webRequestBlocking", "<all_urls>"
    ],
    "author": "https://github.com/gauseen",
    "homepage_url": "https://github.com/gauseen/"
}
